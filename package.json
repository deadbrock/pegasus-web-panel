{
  "name": "pegasus-web-panel",
  "version": "1.0.0",
  "description": "Sistema de Gestão Logística Pegasus - Painel Web",
  "scripts": {
    "dev": "next dev -H 0.0.0.0",
    "build": "next build",
    "start": "next start -H 0.0.0.0",
    "lint": "next lint",
    "type-check": "tsc --noEmit",
